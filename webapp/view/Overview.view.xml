<mvc:View
	controllerName="onlineshop.adminspace.controller.Overview"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
>
	<Page title="{i18n>homePageTitle}">
		<content>
			<f:Card
				class="sapUiMediumMarginTop"
				width="100%"
			>
				<f:header>
					<card:Header
						title="Bestellungen"
						subtitle="Bestellungen anzeigen und versenden"
						iconSrc="sap-icon://sales-order"
						press=".onOrdersPressed"
					/>
				</f:header>
			</f:Card>
			<f:Card
				class="sapUiMediumMarginTop"
				width="100%"
			>
				<f:header>
					<card:Header
						title="Produkte"
						subtitle="Produkte anzeigen, bearbeiten, erstellen und entfernen"
						iconSrc="sap-icon://product"
						press=".onProductsPressed"
					/>
				</f:header>
			</f:Card>
		</content>
	</Page>
</mvc:View>