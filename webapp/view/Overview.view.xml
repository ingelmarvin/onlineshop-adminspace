<mvc:View
	controllerName="onlineshop.adminspace.controller.Overview"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page title="{i18n>homePageTitle}" >
		<content>
			<Button
				width="8rem"
				class="sapUiTinyMargin"
				text="Bestellungen"
				press=".onOrdersPressed"
				icon="sap-icon://sales-order"
			/>
			<Button
				width="8rem"
				class="sapUiTinyMargin"
				text="Produkte"
				press=".onProductsPressed"
				icon="sap-icon://product"
			/>
		</content>
	</Page>
</mvc:View>